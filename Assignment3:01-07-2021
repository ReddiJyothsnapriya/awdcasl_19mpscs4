import java.util.*;
class Solution3
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("n=");
int n=sc.nextInt();
char ch='A';
int i,j;
for(i=1;i<=n;i++)
{
for(j=1;j<=i;j++)
{
System.out.print(ch+" ");
ch++;
}
System.out.println();
}
}
}


Input:
n=5
Output:
A
B C
D E F
G H I J
K L M N O
