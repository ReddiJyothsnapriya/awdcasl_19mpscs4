import java.util.*;
public class Replace
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("n=");
int n=sc.nextInt();
System.out.print("x=");
int x=sc.nextInt();
System.out.print("y=");
int y=sc.nextInt();
int res=0,s=1;
while(n>0) 
{
int rem = n%10;
if(rem == x)
res = res + y * s;
else
res = res + rem * s;
s*= 10;
n /= 10;
}
System.out.print(res);

}
}
