import java.util.*;
public class Patterns1
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("n=");
int n=sc.nextInt();
int i,j;
for(i=1;i<=n;i++)
{
for(j=i;j<=n;j++)
{
System.out.print("*");
}
System.out.println();
}
}
}


Input:
n=5
Output:
*****
****
***
**
*
