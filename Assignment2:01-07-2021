import java.util.*;
class Solution2
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("n=");
int n=sc.nextInt();
int i,j,k;
for(i=1;i<=n;i++)
{
for(j=1;j<=i-1;j++)
{
System.out.print("*");
}
System.out.println();
}
for(i=1;i<=n;i++)
{
for(k=i;k<=n;k++)
{
System.out.print("*");
}
System.out.println();
}
}
}



Input:
n=5
Output:
*
**
***
****
*****
****
***
**
*
