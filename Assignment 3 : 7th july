import java.util.*;
import java.lang.*;
import java.io.*;

class Arrayrotation
{
static void rightrotate(int a[],int n, int k)
{
	k=k%n;

	for(int i = 0; i < n; i++)
	{
        	if(i<k)
		{
			
			System.out.print(a[n + i - k]+ " ");
		}
		else
		{
			System.out.print(a[i - k]+ " ");
		}
	}
	System.out.println();
}
	
public static void main(String args[])
{
	int arr[] = {1,5,6,7,8};
	int n = arr.length;

	int k = 2;
	rightrotate(arr, n, k);

}
}

